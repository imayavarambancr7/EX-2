class Circle {
 private double radius;
 private String color;
 
 public Circle() {
 radius = 1.0;
 color = "red";
 }
 public Circle(double radius) {
 this.radius = radius;
 color = "red";
 }
 public double getRadius() {
 return radius;
 }
 public double getArea() {
 return Math.PI * radius * radius;
 }
 public String getColor() {
 return color;
 }
} 
public class circle {
 public static void main(String[] args) {
 Circle circle1 = new Circle();
 Circle circle2 = new Circle(3.5);
 System.out.println("Circle 1 - Radius: " + circle1.getRadius() + ", Area: " + circle1.getArea() + ", Color: " + 
circle1.getColor());
 System.out.println("Circle 2 - Radius: " + circle2.getRadius() + ", Area: " + circle2.getArea() + ", Color: " + 
circle2.getColor());
 }
}
